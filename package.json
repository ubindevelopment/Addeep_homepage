{
  "name": "addeep-monorepo",
  "private": true,
  "type": "module",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "dev:web": "turbo run dev --filter=web",
    "dev:admin": "turbo run dev --filter=admin",
    "build": "turbo run build",
    "build:web": "turbo run build --filter=web",
    "build:admin": "turbo run build --filter=admin",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "deploy": "./deploy.sh"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "eslint": "^9.23.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "ts-node": "^10.9.2",
    "turbo": "^2.4.4",
    "typescript": "^5.8.2"
  },
  "packageManager": "yarn@4.9.2",
  "resolutions": {
    "react-is": "^19.0.0"
  }
}
