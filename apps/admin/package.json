{
  "name": "admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:report": "playwright show-report",
    "docker:build": "docker build -t admin-dashboard .",
    "docker:run": "docker run -d -p 3001:3001 --name admin-dashboard admin-dashboard",
    "docker:stop": "docker stop admin-dashboard && docker rm admin-dashboard",
    "docker:compose:up": "docker-compose up -d",
    "docker:compose:down": "docker-compose down",
    "docker:compose:logs": "docker-compose logs -f"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.5.0",
    "@mui/material": "^6.5.0",
    "@mui/system": "^6.5.0",
    "@mui/x-date-pickers": "^8.11.1",
    "@mui/x-tree-view": "^8.11.1",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/supabase-js": "^2.57.2",
    "@tanstack/react-query": "^5.87.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "autoprefixer": "^10.4.21",
    "clsx": "^2.1.1",
    "material-react-table": "^3.2.1",
    "next": "^15.2.4",
    "postcss": "^8.5.3",
    "prop-type": "^0.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.0.2",
    "tailwindcss": "^3.4.5",
    "typescript": "^5.8.2",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.0",
    "@types/node": "^24.3.0",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.4"
  }
}
